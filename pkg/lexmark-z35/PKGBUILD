pkgname=lexmark-z35
pkgver=2.0
pkgrel=1
epoch=
pkgdesc="Lexmark Z35 Printer Driver"
arch=('i686' 'x86_64')
url="http://www.lexmark.com/"
license=('custom')
groups=()
depends_i686=('cups' 'libstdc++5')
depends_x86_64=('cups' 'lib32-libcups' 'libstdc++5')
makedepends=('rpmextract' 'gzip')
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
changelog=
#source=("http://downloads.lexmark.com/downloads/cpd/CJLZ35LE-CUPS-2.0-1.TAR.GZ")
source=("http://www.bertrandperrier.fr/linux/CJLZ35LE-CUPS-2.0-1.TAR.GZ")
noextract=()
md5sums=('7260179dce18bef4e50ba06eef51a496')
validpgpkeys=()

build() {
    cd $srcdir
    tar xf CJLZ35LE-CUPS-2.0-1.TAR.GZ
    tail -n +143 lexmarkz35-CUPS-2.0-1.gz.sh > install.tar.gz
    tar xf install.tar.gz
    rpmextract.sh z35llpddk-2.0-2.i386.rpm
    rpmextract.sh lexmarkz35-CUPS-2.0-1.i386.rpm
}

package() {
	cd $srcdir
    mv -f usr $pkgdir
}
